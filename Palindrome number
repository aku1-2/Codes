## Palindrome Number ##
class Solution {
public:
    bool isPalindrome(int x) {
        int d;
        long long rev=0,num=0;
        num=x;
        while(x>0){
            d=x%10;
            rev=rev*10+d;
            x=x/10;
        }
        if(rev==num)
        return true;
        else
        return false;
    }
};
